<?xml version="1.0" encoding="UTF-8"?>
<portlet-app xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_1_0.xsd"
	version="1.0">
	<portlet>
		<portlet-name>LoginPortlet</portlet-name>
		<portlet-class>net.tinyportal.portlet.LoginPortlet</portlet-class>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>VIEW</portlet-mode>
		</supports>
		<supported-locale>fr</supported-locale>
		<security-role-ref>
			<role-name>sbe</role-name>
			<role-link>tomcat</role-link>
		</security-role-ref>
		<security-role-ref>
			<role-name>trustedUser</role-name>
			<role-link>auth-user</role-link>
		</security-role-ref>
	</portlet>
	<portlet>
		<portlet-name>ListAllPortlet</portlet-name>
		<portlet-class>net.tinyportal.portlet.ListAllPortlet</portlet-class>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>VIEW</portlet-mode>
		</supports>
		<supported-locale>fr</supported-locale>
		<resource-bundle>ListAllPortletBundle</resource-bundle>
		<security-role-ref>
			<role-name>sbe</role-name>
			<role-link>tomcat</role-link>
		</security-role-ref>
		<security-role-ref>
			<role-name>trustedUser</role-name>
			<role-link>auth-user</role-link>
		</security-role-ref>
	</portlet>
	<portlet>
		<portlet-name>HtmlPresentationPortlet</portlet-name>
		<portlet-class>net.tinyportal.portlet.HtmlPresentationPortlet</portlet-class>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>VIEW</portlet-mode>
			<portlet-mode>EDIT</portlet-mode>
		</supports>
		<supported-locale>fr</supported-locale>
		<portlet-info>
			<title>Pr√©sentation de tPortal</title>
			<keywords>tPortal presentation</keywords>
		</portlet-info>
	</portlet>
</portlet-app> 
