<?xml version="1.0" encoding="UTF-8"?>
<portlet-app xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_1_0.xsd"
	version="1.0">
	<portlet>
		<portlet-name>DrivePortlet</portlet-name>
		<portlet-class>net.tinyportal.portlet.DrivePortlet</portlet-class>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>VIEW</portlet-mode>
		</supports>
		<supported-locale>fr</supported-locale>
		<security-role-ref>
			<role-name>sbe</role-name>
			<role-link>tomcat</role-link>
		</security-role-ref>
		<security-role-ref>
			<role-name>trustedUser</role-name>
			<role-link>auth-user</role-link>
		</security-role-ref>
	</portlet>
	
	<portlet>
		<portlet-name>PlusPortlet</portlet-name>
		<portlet-class>net.tinyportal.portlet.PlusPortlet</portlet-class>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>VIEW</portlet-mode>
		</supports>
		<supported-locale>fr</supported-locale>
		<security-role-ref>
			<role-name>sbe</role-name>
			<role-link>tomcat</role-link>
		</security-role-ref>
		<security-role-ref>
			<role-name>trustedUser</role-name>
			<role-link>auth-user</role-link>
		</security-role-ref>
	</portlet>
</portlet-app> 
